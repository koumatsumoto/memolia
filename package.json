{
  "name": "memolia",
  "version": "0.0.0",
  "scripts": {
    "build": "ng build",
    "check:diff": "git diff --exit-code --name-only .",
    "ci": "yarn format && yarn check:diff && yarn build && yarn ci:check:test",
    "ci:check:test": "yarn test:unit --ci --silent --detectOpenHandles --forceExit",
    "e2e": "ng e2e",
    "format": "yarn :lint:ts --fix && yarn :lint:scss --fix && yarn :lint:ng --fix && prettier --config .prettierrc --write '{.circleci,docs,e2e,src}/**/*.{ts,js,json,scss,html,md,yml}' ./*.{md,js,json}",
    "jest": "node --max_old_space_size=4096 ./node_modules/.bin/jest",
    "lint": "yarn :lint:ng && :lint:ts && :lint:scss",
    "ng": "ng",
    "start": "ng serve",
    "test:unit": "yarn jest -c jest.config.js",
    ":lint:ng": "ng lint",
    ":lint:scss": "stylelint 'src/**/*.scss'",
    ":lint:ts": "eslint 'src/**/*.ts'"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~10.0.5",
    "@angular/common": "~10.0.5",
    "@angular/compiler": "~10.0.5",
    "@angular/core": "~10.0.5",
    "@angular/forms": "~10.0.5",
    "@angular/platform-browser": "~10.0.5",
    "@angular/platform-browser-dynamic": "~10.0.5",
    "@angular/router": "~10.0.5",
    "rxjs": "~6.5.4",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1000.4",
    "@angular/cli": "~10.0.4",
    "@angular/compiler-cli": "~10.0.5",
    "@angular/language-service": "~10.0.5",
    "@types/jest": "^25.1.4",
    "@types/node": "^13.9.2",
    "@typescript-eslint/eslint-plugin": "^2.24.0",
    "@typescript-eslint/parser": "^2.24.0",
    "codelyzer": "^6.0.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-prettier": "^3.1.2",
    "jest": "^25.1.0",
    "jest-preset-angular": "^8.1.2",
    "prettier": "^1.19.1",
    "protractor": "~7.0.0",
    "stylelint": "^13.2.1",
    "stylelint-config-recess-order": "^2.0.4",
    "stylelint-config-recommended-scss": "^4.2.0",
    "stylelint-scss": "^3.16.0",
    "ts-node": "~8.7.0",
    "tslint": "~6.1.0",
    "typescript": "~3.9.7"
  }
}
